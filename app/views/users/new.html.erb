<div id="container">
  <div class="row">
    <div class="col s8 offset-s2">
<%= form_for(@user) do |f| %>  
  <div class="field"> 
    <%= f.label :first_name %><br> 
    <%= f.text_field :first_name %> 
  </div> 
  <div class="field"> 
    <%= f.label :last_name %><br> 
    <%= f.text_field :last_name %> 
  </div> 
  <div class="field"> 
    <%= f.label :email %><br> 
    <%= f.email_field :email %> 
  </div> 
  <div class="field"> 
    <%= f.label :password %><br> 
    <%= f.password_field :password %> 
  </div> 
  <div class="actions"> 
    <%= f.submit "Create" %> 
  </div> 
<% end %>
</div>
</div>
</div>